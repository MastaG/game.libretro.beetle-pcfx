<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.beetle-pcfx"
		name="PC-FX (Mednafen PC-FX)"
		version="0.9.33"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>PCXFX</platforms>
		<extensions>cue|ccd|toc</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<supports_keyboard>false</supports_keyboard>
		<supports_mouse>false</supports_mouse>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">PC-FX (Mednafen PC-FX) v0.9.33.3</summary>
		<description>Supported files: .cue, .ccd, .toc</description>
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
	</extension>
</addon>